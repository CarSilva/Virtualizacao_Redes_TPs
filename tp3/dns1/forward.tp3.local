;
; SOA
;
$TTL 1h
@   IN SOA dns1.tp3.local. root.tp3.local. (
        1 ; Serial number (YYYYMMDDnn)
        1h ; Slave refresh
        15m ; Slave retry
        2w ; Slave expire
        1h ; Cache TTL
     )
;
; NS RECORDS
;
@   IN   NS  dns1.tp3.local.
;
; A RECORDS
;

tp3.local.    IN A 10.0.0.249
@       			IN A 10.0.0.249
dns1         	IN A 10.0.0.249
